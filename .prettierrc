{
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"^react(.*)",
		"^@emotion/(.*)$",
		"^@tanstack/(.*)$",
		"^axios$",
		"^[^@./]",
		"^~/",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderParserPlugins": ["jsx", "typescript"],
	"printWidth": 100,
	"tabWidth": 4,
	"useTabs": true,
	"prettier.printWidth": 400
}
